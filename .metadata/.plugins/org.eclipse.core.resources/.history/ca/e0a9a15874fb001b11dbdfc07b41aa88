package com.syntax.class10;

public class HW {

	   public static void main(String[] args) {

	   int i = 50;

	     int[] a = new int[10];

	     System.out.println(a.length);

	     a = new int[100];

	     System.out.println(a.length);

	     a = new int[i];

	     System.out.println(a.length);

	  }}

