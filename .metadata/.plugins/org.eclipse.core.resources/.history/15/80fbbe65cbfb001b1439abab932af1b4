package com.syntax.reviewclass4;

public class NestedLoops {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		
		
		//11111
		//22222
        //33333
		//44444
		//55555
		
		//54321
		//54321
		//54321
		//54321
		
 for (int j=1; j<6; j++) {	
 for (int i=0; i<5 ; i++) {
		
		System.out.print(j);}
		System.out.println( );

		
		System.out.println("----------------------------------" );	
		
		for (int a=0; a<5; a++) {	
		 for (int b=5; b>0 ; b--) {
					
					System.out.print(a);}
					System.out.println( );	
		
		
		
		

}}}}